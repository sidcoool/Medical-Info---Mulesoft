#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    patientID:
      type: number
      required: false
    lastname:
      type: string
      required: true
      maxLength: 50
    age:
      type: number
      required: true
    address:
      type: string
      maxLength: 100
      required: true
    city:
      type: string
      maxLength: 50
      required: true
    dob:
      type: date-only
      required: false
  